{
  "hosting": {
    "source": ".",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "frameworksBackend": {
      "region": "asia-east1"
    },
    "headers": [
      {
        "source": "**/*.{jpg,jpeg,gif,png,webp,svg,js,css,woff,woff2,ttf,eot}",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=86400, immutable"
          }
        ]
      }
    ]
  }
}
